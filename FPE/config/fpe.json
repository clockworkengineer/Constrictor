{
    "plugins": [
        "plugins.fileannouncer_handler"
    ],
    "watchers": [
        {
            "name": "File Copier 1",
            "type": "CopyFile",
            "source": ".\\watcher\\source1\\",
            "destination": ".\\watcher\\destination1\\",
            "deletesource": false,
            "exitonfailure": true,
            "recursive": true
        }
    ],
    "logging": {
        "filename": ".\\fpe.log",
        "filemode": "a",
        "level": 10,
        "format": "%(asctime)s:%(message)s"
    }
}