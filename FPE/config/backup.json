{
    "plugins": [
        "plugins.fileannouncer_handler"
    ],
    "watchers": [
        {
            "name": "File Copier 1",
            "type": "CopyFile",
            "source": ".\\watcher\\source1\\",
            "destination": ".\\watcher\\destination1\\",
            "deletesoure": true,
            "exitonfailure": false,
            "recursive": false
        },
        {
            "name": "File Copier 2",
            "type": "CopyFile",
            "source": ".\\watcher\\source2\\",
            "destination": ".\\watcher\\destination2\\",
            "deletesoure": true,
            "exitonfailure": false,
            "recursive": false
        },
        {
            "name": "File Copier 3",
            "type": "CopyFile",
            "source": ".\\watcher\\source3\\",
            "destination": ".\\watcher\\destination3\\",
            "deletesoure": true,
            "exitonfailure": false,
            "recursive": false
        }
    ],
    "logging": {
        "filename": ".\\fpe.log",
        "filemode": "a",
        "level": 20,
        "format": "%(asctime)s:%(message)s"
    }
}