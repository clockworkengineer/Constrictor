{
    "plugins": [
        "plugins.fileannouncer_handler"
    ],
    "watchers": [
        {
            "name": "File Copier 1",
            "type": "CSVFileToSQLite",
            "source": ".\\watcher\\source1\\",
            "deletesource": true,
            "exitonfailure": true,
            "recursive": false,
            "databasefile": ".\\sqlite\\users",
            "key": "id",
            "table": "details"
        }
    ],
    "logging": {
        "filename": ".\\fpe.log",
        "filemode": "a",
        "level": 10,
        "format": "%(asctime)s:%(message)s"
    }
}