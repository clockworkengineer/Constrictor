{
    "plugins": [
        "plugins.fileannouncer"
    ],
    "watchers": [
        {
            "name": "File Copier",
            "type": "CopyFile",
            "watch": "c:\\temp\\watch",
            "destination": "c:\\temp\\destination",
            "recursive": true
        }
    ],
    "logging": {
        "level": 20,
        "format": "%(asctime)s:%(message)s"
    }
}